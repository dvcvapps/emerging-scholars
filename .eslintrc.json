{
    "extends": ["airbnb"],
    "parserOptions": {
        "sourceType": "module"
    },
    "ignorePatterns": ["reportWebVitals.js"],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "react/jsx-filename-extension": [1, { "allow": "as-needed" }]
    }
  }